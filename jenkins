pipeline{
  agent{
    docker {image 'java:latest' }
    }
    stages{
      stage('Checkout'){
          git 'https://github.com/ali-057/java'
      }
      stage('Compile and  run java program')  {
        steps  {
          sh 'javac hello.java'
          sh 'java hello'
        }
      }
    }
}

