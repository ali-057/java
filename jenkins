pipeline{
  agent{
    docker {image 'openjdk:11' }
    }
    stages{
      stage('Checkout'){
          steps{
              git 'https://github.com/ali-057/java'
          } 
      }
      stage('Compile and  run java program')  {
        steps  {
          sh 'javac hello.java'
          sh 'java hello'
        }
      }
    }
}


